<h1>Movie Results</h1>

    <% if (typeof data["Search"] !== "undefined") { %>
        <% data["Search"].forEach(function(movie) { %>
        <ul>
            <li>
                <strong><%= movie["Title"] %></strong> - <%= movie["Year"] %>
            </li>
        </ul>
        <% }); %>

    <% } else { %>
        <h2>Hmm...I'm not familiar with that movie. Try another title!</h2>
    <% } %>

<button><a href="/">Search Again</a></button>